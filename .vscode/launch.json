    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "common_core/philosophers/philo",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/workspace/push_swap/a.out",
            "args": ["-334","281","-995","-847","285","-136","-281","-48","624","416","378","-981","525","278","-965","-612","-364","-272","296","-651","519","810","-819","463","-728","193","98","-462","56","-991","-117","874","-219","785","867","-153","771","-381","-909","409","43","534","481","62","-214","-298","295","-8","687","-154","-937","-395","505","510","408","-980","-703","-224","759","-561","-560","595","-433","438","994","803","668","995","788","34","102","-768","-46","528","-241","144","-338","-102","244","-407","411","-60","837","300","722","713","-814","-362","-127","-806","388","-895","-455","696","279","547","-667","-120","-863","-310"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "tasks",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],  
        }

    ]
