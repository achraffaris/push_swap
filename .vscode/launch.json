    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "common_core/philosophers/philo",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/workspace/push_swap/a.out",
            "args": ["69","91","14","96","26","65","54","92","27","41","16","76","73","86","34","24","58","12","80","30","45","36","21","1","62","42","83","55","22","59","18","23","49","93","44","4","88","53","68","50","82","75","13","94","46","37","28","10","47","71","63","85","89","77","87","56","5","43","31","61","33","52","2","38","84","7","72","19","67","48","90","35","57","95","64","40","3","98","8","81","29","15","9","51","60","70","20","39","100","66","97","99","78","11","6","25","32","79","74","17"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "tasks",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
